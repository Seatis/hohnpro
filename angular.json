{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hohnpro": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/backend",
              "src/favicon.ico"
            ],
            "styles": [
              "src/assets/font/linea-basic/styles.css",
              "src/assets/font/linea-ecommerce/styles.css",
              "src/assets/font/linea-arrows/styles.css",
              "src/assets/css/jquery.fancybox.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/fontello.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/stylem.css",
              "src/assets/css/responsive.css",
              "src/assets/css/owl.theme.css",
              "src/assets/css/owl.transitions.css",
              "src/assets/css/audioplayer.css",
              "src/styles.css",
              "node_modules/primeng/resources/themes/nova-light/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/cookieconsent/build/cookieconsent.min.css"
            ],
            "scripts": [
                "src/assets/js/jquery.modernizr.js",
                "src/assets/js/jquery-2.2.4.min.js",
                "src/assets/js/jquery-ui.min.js",
                "src/assets/js/retina.min.js",
                "src/assets/js/instafeed.min.js",
                "src/assets/js/jquery.tweet.js",
                "src/assets/js/jquery.queryloader2.min.js",
                "src/assets/js/jquery.fancybox.min.js",
                "src/assets/js/owl.carousel.min.js",
                "src/assets/js/bootstrap.js",
                "src/assets/js/plugins.js",
                "src/assets/js/script.js",
                "src/assets/js/lightwidget.js",
                "node_modules/cookieconsent/build/cookieconsent.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/backend/mysql_conn_names.php",
                  "with": "src/backend_prod/mysql_conn_names.php"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hohnpro:build",
            "port": 6300
          },
          "configurations": {
            "production": {
              "browserTarget": "hohnpro:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hohnpro:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hohnpro-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "hohnpro:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "hohnpro",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
